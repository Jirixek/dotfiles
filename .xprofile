#!/bin/sh


# System bar
dwmblocks &
nm-applet &
redshift-gtk &

# Deamons
unclutter &
udiskie &
dunst &
background &

# Peripherals
numlockx on &
xset r rate 350 50 &
xrdb -load "${HOME}/.Xresources" &

# Other
rfkill block bluetooth &
picom -CGb --experimental-backends --config="${HOME}"/.config/picom/picom_"$(hostname)".conf &

# tmux
tmux-init &
